{
	"name": "markdown-it-collapsible",
	"version": "1.0.0",
	"description": "A markdown-it plugin, which adds collapsibles via the HTML <details> and <summary> elements",

	"main": "dist/markdown-it-collapsible.cjs.js",
	"module": "dist/markdown-it-collapsible.es.js",
	"exports": {
		"require": "./dist/markdown-it-collapsible.cjs.js",
		"import": "./dist/markdown-it-collapsible.es.js",
		"default": "./dist/markdown-it-collapsible.es.js"
	},

	"types": "./dist/src/plugin.d.ts",
	"scripts": {
		"build": "vite build",
		"watch": "vite build --watch",
		"test": "vitest",
		"lint": "eslint src --ext ts --fix --no-error-on-unmatched-pattern",
		"lint:tsc": "tsc --noEmit"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/bioruebe/markdown-it-collapsible"
	},
	"author": "bioruebe",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/bioruebe/markdown-it-collapsible/issues"
	},
	"homepage": "https://github.com/bioruebe/markdown-it-collapsible",
	"keywords": [
		"markdown",
		"markdown-it",
		"markdown-it-plugin"
	],
	"devDependencies": {
		"@types/markdown-it": "^12.2.3",
		"@types/node": "14",
		"@typescript-eslint/eslint-plugin": "^5.10.2",
		"@typescript-eslint/parser": "^5.10.2",
		"eslint": "^8.8.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-unicorn": "^40.1.0",
		"markdown-it": "^12.3.2",
		"markdown-it-testgen": "^0.1.6",
		"mocha": "^9.2.0",
		"prettier": "^2.5.1",
		"typescript": "^4.5.5",
		"vite": "^2.7.13",
		"vite-plugin-dts": "^0.9.9",
		"vitest": "^0.2.5"
	},
	"peerDependencies": {
		"markdown-it": ">=12"
	}
}
